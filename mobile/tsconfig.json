{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "~/components/*": [
        "./src/components/*"
      ],
      "~/assets/*": [
        "./assets/*"
      ],
      "~/hooks/*": [
        "./src/hooks/*"
      ],
      "~/screens/*": [
          "./src/screens/*"
      ],
      "~/contexts/*": [
          "./src/contexts/*"
      ],
      "~/utils/*": [
          "./src/utils/*"
      ],
    }
  }
}
