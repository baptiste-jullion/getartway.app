    "js-yaml": "^4.1.0",
    "pg": "^8.16.0"
  },
  "devDependencies": {
    "@types/pg": "^8.15.1",
    "bun-types": "latest",
    "prisma": "^6.7.0",
    "tsc-alias": "^1.8.16"
  }
}
