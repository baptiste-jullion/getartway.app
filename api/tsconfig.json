{
  "compilerOptions": {
    "target": "ES2021",
    "module": "ES2022",
    "moduleResolution": "node",
    "baseUrl": "./",
    "paths": {
      "#utils": [
        "src/utils"
      ],
      "#controllers/*": [
        "src/controllers/*"
      ],
      "#services/*": [
        "src/services/*"
      ],
      "#dtos/*": [
        "src/dtos/*"
      ],
      "#models": [
        "generated/prisma"
      ],
      "#database": [
        "src/database/index"
      ],
      "#index": [
        "src/index"
      ],
      "#plugins/*": [
        "src/plugins/*"
      ],
    },
    "types": [
      "bun-types"
    ],
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}